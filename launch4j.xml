<launch4jConfig>
  <headerType>gui</headerType>
  <outfile>bin/AlexJ.exe</outfile>
  <jar>bin/AlexJ.jar</jar>
  <dontWrapJar>false</dontWrapJar>

  <errTitle>AlexJ</errTitle>

  <stayAlive>true</stayAlive>
  <chdir>.</chdir>

  <jre>
    <path>.\JRE</path>
    <minVersion>1.5.0</minVersion>
    <maxHeapSize>192</maxHeapSize>
  </jre>

  <splash>
    <file>splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
  </splash>


</launch4jConfig>
